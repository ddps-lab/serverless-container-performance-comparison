# 모델 서버의 주소 및 포트 번호, 모델 이름 설정

# DNS, IP 주소만 작성합니다. (기본 포트번호 443)
# EX) serving-host
grpc_server_address = ''
# gRPC에서 https 사용시 1로 설정, http 사용시 0으로 설정
use_https = 1
# protocol을 포함한 URI 형태로 작성
# EX) https://serving-host/
rest_server_address = ''
# gRPC bench 결과를 저장할 google spread sheet id
grpc_spreadsheet_id = ''
# REST bench 결과를 저장할 google spread sheet id
rest_spreadsheet_id = ''
# 추론을 테스트할 모델 이름
model_name = ''
# 추론 횟수 지정 (병렬로 한번에 처리됩니다.)
num_tasks = 10